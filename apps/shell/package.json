{
  "name": "shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "serve:prod": "ng serve --configuration=production",
    "build": "npm run prerender",
    "watch": "ng build --watch --configuration=development",
    "test": "ng test",
    "dev:ssr": "ng run shell:serve-ssr",
    "serve:ssr": "node dist/shell/server/main.js",
    "build:ssr": "ng build && ng run shell:server",
    "prerender": "ng run shell:prerender",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "deploy": "firebase deploy",
    "clean": "rimraf .angular && rimraf node_modules && rimraf dist"
  },
  "private": true,
  "dependencies": {
    "@youtube/common-ui": "*"
  }
}
