{
  "name": "root",
  "private": true,
  "packageManager": "npm@8.5.3",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "serve": "concurrently \"turbo run pack-lib\"  \"turbo run serve  --no-cache  --continue\"",
    "serve:prod": "concurrently \"turbo run pack-lib:prod\"  \"turbo run serve:prod --no-cache --continue\"",
    "build": "turbo run build --no-cache",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "clean": "turbo run clean && rimraf node_modules --continue",
    "deploy:fe": "turbo run build --no-cache --concurrency=1 --stream --ignore=*backend* && turbo run deploy --ignore=*backend* --concurrency=1 --stream",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.2",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "13.2.2",
    "@angular/compiler-cli": "13.2.2",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nguniversal/builders": "13.0.2",
    "@schematics/angular": "13.2.2",
    "@types/express": "4.17.13",
    "@types/jasmine": "~3.10.0",
    "@types/jest": "27.4.0",
    "@types/node": "^12.11.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "concurrently": "7.0.0",
    "cross-port-killer": "1.4.0",
    "eslint": "^8.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jasmine-core": "~4.0.0",
    "jest": "^27.2.5",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lerna": "4.0.0",
    "minimist": "1.2.5",
    "ng-packagr": "^13.0.0",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "turbo": "1.1.4",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@angular-architects/module-federation": "14.0.1",
    "@angular/animations": "13.2.2",
    "@angular/cdk": "13.2.2",
    "@angular/common": "13.2.2",
    "@angular/compiler": "13.2.2",
    "@angular/core": "13.2.2",
    "@angular/elements": "13.2.2",
    "@angular/forms": "13.2.2",
    "@angular/material": "13.2.2",
    "@angular/platform-browser": "13.2.2",
    "@angular/platform-browser-dynamic": "13.2.2",
    "@angular/platform-server": "13.2.2",
    "@angular/router": "13.2.2",
