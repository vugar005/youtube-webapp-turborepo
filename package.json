    "@ngrx/store": "13.0.2",
    "@ngrx/store-devtools": "13.0.2",
    "@nguniversal/express-engine": "13.0.2",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "express": "4.17.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.4.0",
    "tslib": "^2.3.0",
    "youtube-search-without-api-key": "1.0.7",
    "youtube-sr": "4.1.15",
    "zone.js": "~0.11.4"
  }
}
