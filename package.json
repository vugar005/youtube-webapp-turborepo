    "lerna": "4.0.0",
    "minimist": "1.2.5",
    "ng-packagr": "14.2.0",
    "ngx-build-plus": "14.0.0",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "turbo": "1.2.8",
    "typescript": "4.6.4",
    "webpack-bundle-analyzer": "4.5.0"
  }
}
