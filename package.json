{
  "name": "root",
  "private": true,
  "packageManager": "npm@8.5.3",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "serve": "turbo run serve  --no-cache  --continue",
    "serve:prod": "turbo run serve:prod --no-cache --continue",
    "build": "turbo run build --no-cache",
    "lint": "turbo run lint",
    "lint:changed": "turbo run lint:fix --filter=[HEAD]",
    "lint:fix": "turbo run lint:fix",
    "clean": "turbo run clean && rimraf node_modules --continue",
    "clean:cache": "turbo run clean",
    "deploy:fe": "turbo run build --no-cache --concurrency=85% --filter=!backend && turbo run deploy --filter=!backend --concurrency=1",
    "prepare": "husky install",
    "analyze": "turbo run analyze --filter=shell"
  },
  "dependencies": {
    "@angular-architects/module-federation": "14.3.10",
    "@angular-architects/module-federation-tools": "14.3.10",
    "@angular/animations": "14.2.0",
    "@angular/cdk": "14.2.0",
    "@angular/common": "14.2.0",
    "@angular/compiler": "14.2.0",
    "@angular/core": "14.2.0",
    "@angular/elements": "14.2.0",
    "@angular/forms": "14.2.0",
    "@angular/material": "14.2.0",
    "@angular/platform-browser": "14.2.0",
    "@angular/platform-browser-dynamic": "14.2.0",
    "@angular/platform-server": "14.2.0",
    "@angular/router": "14.2.0",
    "@angular/youtube-player": "14.1.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@ng-web-apis/common": "2.0.0",
    "@ngrx/store": "14.3.0",
    "@ngrx/store-devtools": "14.3.0",
    "@nguniversal/express-engine": "14.0.3",
    "express": "4.17.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.5.6",
    "tslib": "^2.3.0",
    "youtube-search-without-api-key": "1.0.7",
    "youtube-sr": "4.1.15",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.1",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "14.2.1",
    "@angular/compiler-cli": "14.2.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nguniversal/builders": "14.0.3",
    "@schematics/angular": "14.2.1",
    "@types/express": "4.17.13",
    "@types/jasmine": "~3.10.0",
    "@types/jest": "27.4.0",
    "@types/node": "^12.11.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "concurrently": "7.0.0",
    "cross-port-killer": "1.4.0",
    "eslint": "^8.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jasmine-core": "~4.0.0",
    "jest": "^27.2.5",
